{
  "name": "ud-imuv",
  "version": "1.3.0",
  "description": "Web application of urban data based on ud-viz, jitsi, parse and whitebophir",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -f -r node_modules && rm -f package-lock.json && rm -f  -r ./public/dist/",
    "clear-node": "sudo killall -9 node",
    "reset": "npm run clean && npm i",
    "assert-code": "cross-env NODE_ENV=production node ./bin/assertCode.js",
    "validate-links": "node ./bin/validateLinks.js",
    "eslint": "eslint --ignore-path .gitignore \"packages/**/*.js\"",
    "eslint-quiet": "eslint --quiet --ignore-path .gitignore \"packages/**/*.js\" ",
    "eslint-fix": "eslint --fix --ignore-path .gitignore \"packages/**/*.js\" ",
    "local-ci": "npm run eslint && npm run assert-code && npm audit --audit-level=low && npm run validate-links",
    "build-dev": "dotenv -e .env -- cross-env NODE_ENV=development webpack --config ./webpack.config.js",
    "dev": "dotenv -e .env -- cross-env NODE_ENV=development nodemon --trace-warnings --verbose --watch ./src --watch ./bin/backend --delay 2500ms ./bin/dev.js -e js,css,html",
    "host": "dotenv -e .env -- node ./bin/host.js",
    "analyze-bundle-game": "cross-env ANALYZE=analyze NAME=game ENTRY=./src/browser/game/index.js npm run build-dev",
    "create-admin": "dotenv -e .env node ./bin/backend/createAdmin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VCityTeam/UD-Imuv.git"
  },
  "keywords": [
    "ud-viz"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VCityTeam/UD-Imuv/issues"
  },
  "homepage": "https://github.com/VCityTeam/UD-Imuv#readme",
  "devDependencies": {
    "@ud-viz/game_node": "file:../UD-Viz/packages/game_node",
    "@ud-viz/game_node_template": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "child-process-promise": "2.2.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "dotenv": "16.0.1",
    "dotenv-cli": "7.2.1",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-prettier": "4.2.1",
    "express": "^4.18.2",
    "front-matter-loader": "^2.0.0",
    "gitlog": "4.0.4",
    "html-loader": "2.1.2",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "parse": "^4.2.0",
    "prettier": "2.7.1",
    "raw-loader": "4.0.2",
    "reload": "3.2.1",
    "remark-cli": "^11.0.0",
    "remark-validate-links": "^12.1.1",
    "string-replace-middleware": "1.0.2",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@ud-viz/frame3d": "file:../UD-Viz/packages/frame3d",
    "@ud-viz/game_browser": "file:../UD-Viz/packages/game_browser",
    "@ud-viz/game_browser_template": "file:../UD-Viz/packages/game_browser_template",
    "@ud-viz/game_shared": "file:../UD-Viz/packages/game_shared",
    "@ud-viz/game_shared_template": "file:../UD-Viz/packages/game_shared_template",
    "@ud-viz/utils_browser": "4.0.0",
    "@ud-viz/widget_3d_tiles": "^4.0.0",
    "@ud-viz/widget_camera_positioner": "4.0.0",
    "@ud-viz/widget_geocoding": "4.0.0",
    "@ud-viz/widget_layer_choice": "4.0.0",
    "buffer": "^6.0.3",
    "itowns": "2.40.0",
    "jitsi-iframe-api": "^1.0.0",
    "jquery": "^3.7.1",
    "proj4": "^2.8.0",
    "three": "^0.154.0"
  }
}
