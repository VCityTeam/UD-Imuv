{
  "name": "server",
  "version": "1.0.0",
  "description": "application hosting a imuv client",
  "main": "index.js",
  "scripts": {
    "travis": "npm run eslint && npm run build",
    "eslint": "./node_modules/.bin/eslint ./src",
    "eslint-fix": "./node_modules/.bin/eslint ./src --fix",
    "build": "cross-env NODE_ENV=production webpack",
    "build-debug": "cross-env NODE_ENV=development webpack",
    "debug": "nodemon --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html",
    "clear-node": "sudo killall -9 node"
  },
  "keywords": [
    "game"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "firebase": "^8.5.0",
    "gm": "^1.23.1",
    "pngjs": "^6.0.0",
    "socket.io": "^4.0.0",
    "ud-viz": "^2.32.0"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "nodemon": "^2.0.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
