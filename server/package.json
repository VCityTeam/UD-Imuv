{
  "name": "server",
  "version": "1.3.0",
  "description": "application hosting a imuv client",
  "main": "index.js",
  "scripts": {
    "travis": "npm run eslint && npm run build",
    "eslint": "eslint ./src",
    "eslint-fix": "eslint ./src --fix",
    "format-worlds": "prettier --write ../client/assets/worlds/*",
    "build": "cross-env NODE_ENV=production webpack",
    "build-debug": "cross-env NODE_ENV=development webpack",
    "debug": "nodemon --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html",
    "clear-node": "sudo killall -9 node"
  },
  "keywords": [
    "game"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "gm": "^1.23.1",
    "parse": "^3.4.1",
    "pngjs": "^6.0.0",
    "socket.io": "^4.0.0",
    "ud-viz": "^2.39.2"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.7.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
