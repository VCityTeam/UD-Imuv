{
  "name": "imuv-client",
  "version": "1.3.0",
  "description": "udv game client",
  "main": "./src/bootstrap.js",
  "scripts": {
    "travis": " npm run eslint && npm run build",
    "eslint": "eslint ./src",
    "eslint-fix": "eslint ./src --fix",
    "parseWorlds": "node ./bin/parseWorlds.js",
    "parseGitLog": "node ./bin/parseGitLog.js",
    "build": "npm run parseGitLog && cross-env NODE_ENV=production webpack",
    "build-debug": "npm run parseGitLog && cross-env NODE_ENV=development webpack",
    "debug": "nodemon --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@egjs/hammerjs": "^2.0.17",
    "jitsi-iframe-api": "^1.0.0",
    "keycharm": "^0.4.0",
    "quickhull": "^1.0.3",
    "showdown": "^2.1.0",
    "timsort": "^0.3.0",
    "ud-viz": "^2.39.2",
    "uuid": "^8.3.2",
    "vis-util": "^5.0.2"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "front-matter-loader": "^2.0.0",
    "gitlog": "^4.0.4",
    "html-loader": "^2.1.2",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.5.0"
  }
}
