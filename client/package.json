{
  "name": "imuv-client",
  "version": "1.1.0",
  "description": "udv game client",
  "main": "./src/bootstrap.js",
  "scripts": {
    "prettier": "npx prettier --single-quote --write ./assets ./src",
    "travis": "npm run prettier && npm run eslint && npm run build",
    "travis-ps": "npm run prettier && npm run eslint-ps && npm run build",
    "eslint": "./node_modules/.bin/eslint ./src",
    "eslint-ps": "eslint ./src",
    "eslint-fix": "./node_modules/.bin/eslint ./src --fix",
    "eslint-fix-ps": "eslint ./src --fix",
    "parseWorlds": "node ./bin/parseWorlds.js",
    "parseGitLog": "node ./bin/parseGitLog.js",
    "build": "npm run parseGitLog && cross-env NODE_ENV=production webpack",
    "build-debug": "npm run parseGitLog && cross-env NODE_ENV=development webpack",
    "debug": "nodemon --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@egjs/hammerjs": "^2.0.17",
    "keycharm": "^0.4.0",
    "quickhull": "^1.0.3",
    "timsort": "^0.3.0",
    "ud-viz": "2.37.3",
    "uuid": "^8.3.2",
    "vis-util": "^5.0.2"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.10",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "gitlog": "^4.0.4",
    "html-loader": "^2.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.6.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  }
}
