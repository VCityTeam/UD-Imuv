    "vis-util": "^5.0.2"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.10",
    "eslint": "^7.28.0",
    "html-loader": "^2.1.2",
    "nodemon": "^2.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
